---
import Layout from '../layouts/Layout.astro';
import ViewHeader from '../components/sections/ViewHeader.astro';
import SectionHeader from '../components/sections/SectionHeader.astro'
import SectionModelY from '../components/sections/SectionModelY.astro';
import SectionModel3 from '../components/sections/SectionModel3.astro';
import SectionModelS from '../components/sections/SectionModelS.astro';
import SectionModelX from '../components/sections/SectionModelX.astro';
import SectionSolar from '../components/sections/SectionSolar.astro';
import SectionAccesorios from '../components/sections/SectionAccesorios.astro';
---
<Layout title="Tesla Landing">
    <ViewHeader/>
    <main class="snap-mandatory snap-y overflow-y-auto overflow-x-hidden scroll-smooth h-screen w-full">
        <div class="snap-center">
            <SectionHeader/>
        </div>
        <div class="snap-center">
            <SectionModelY/>
        </div>
        <div class="snap-center">
            <SectionModel3/>
        </div>
        <div class="snap-center">
            <SectionModelS/>
        </div>
        <div class="snap-center">
            <SectionModelX/>
        </div>
        
        <div class="snap-center">
            <SectionSolar/>
        </div>
        <div class="snap-center">
            <SectionAccesorios/>
        </div>
    </main>
</Layout>
